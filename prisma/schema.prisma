generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  date     DateTime @default(now())
  phone    String
  address  String
  username String
  password String
}
